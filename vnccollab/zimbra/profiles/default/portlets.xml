<?xml version="1.0"?>
<portlets
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="vnccollab.zimbra">

 <portlet
   addview="vnccollab.zimbra.portlets.ZimbraMailPortlet"
   title="Zimbra: Mail Portlet"
   description="A portlet which can render Zimbra Mail boxes."
   i18n:attributes="title;description">
   <for interface="plone.app.portlets.interfaces.IColumn" />
   <for interface="plone.app.portlets.interfaces.IDashboard" />
 </portlet>

 <portlet
   addview="vnccollab.zimbra.portlets.ZimbraCalendarPortlet"
   title="Zimbra: Calendar Portlet"
   description="A portlet which can render Zimbra Calendar."
   i18n:attributes="title;description">
   <for interface="plone.app.portlets.interfaces.IColumn" />
   <for interface="plone.app.portlets.interfaces.IDashboard" />
 </portlet>

</portlets>
